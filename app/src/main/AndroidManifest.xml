<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="io.jetstudio.drawerbehaviorexample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">

        <activity android:name="io.jetstudio.drawerbehaviorexample.MainActivity"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="io.jetstudio.drawerbehaviorexample.drawer.DefaultDrawerActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="io.jetstudio.drawerbehaviorexample.drawer.AdvanceDrawer1Activity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="io.jetstudio.drawerbehaviorexample.drawer.AdvanceDrawer2Activity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="io.jetstudio.drawerbehaviorexample.drawer.AdvanceDrawer3Activity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="io.jetstudio.drawerbehaviorexample.drawer.AdvanceDrawer4Activity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="io.jetstudio.drawerbehaviorexample.drawer.AdvanceDrawer5Activity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="io.jetstudio.drawerbehaviorexample.drawer.AdvanceDrawer6Activity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="io.jetstudio.drawerbehaviorexample.drawer.Advance3DDrawer1Activity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>